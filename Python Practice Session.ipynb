    "import locale\n",
    "import datetime\n",
    "\n",
    "today = datetime.datetime.now().date()\n",
    "\n",
    "locale.setlocale(locale.LC_ALL, 'en_US')\n",
    "\n",
    "a = 10\n",
    "b = 7\n",
    "\n",
    "total = a + b\n",
    "\n",
    "\n",
    "\n",
    "print(f'Your total is: {locale.currency(total)}! purchased on {today}')"
